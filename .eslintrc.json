{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    // "extends": "eslint:recommended",    
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
    "no-extra-parens": [
        "error",
        "all",
        {
            "nestedBinaryExpressions": false
        }
    ],
    "no-prototype-builtins": 2,
    "no-template-curly-in-string": 2,
    "valid-jsdoc": 2,
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "default-case": [
        2,
        {
            "commentPattern": "^skip\\sdefault"
        }
    ],
    // "guard-for-in": 2,
    "no-caller": 2,
    "no-empty-function": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-param-reassign": [
        1
    ],
    "no-proto": 2,
    "no-return-assign": 2,
    "no-return-await": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 2,
    "no-unused-expressions": [2, {
            "allowTernary": true
        }],
    "no-useless-concat": 2,
    "no-useless-call": 2,
    "no-useless-return": 2,
    "no-void": 2,
    "no-with": 2,
    "prefer-promise-reject-errors": 2,
    "require-await": 2,
    "vars-on-top": 2,
    "wrap-iife": 2,
    "no-delete-var": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-use-before-define": [
        2,
        {
            "functions": true
        }
    ],
    "callback-return": [
        2,
        [
            "callback",
            "cb",
            "next",
            "done"
        ]
    ],
    "global-require": 2,
    "handle-callback-err": 2,
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-process-env": 2,
    "no-process-exit": 2,
    "no-sync": 1,
    "array-bracket-spacing": [
        2,
        "never"
    ],
    "block-spacing": [
        2,
        "always"
    ],
    "brace-style": [
        2,
        "1tbs",
        {
            "allowSingleLine": true
        }
    ],
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": [
        2,
        "never"
    ],
    "eol-last": 2,
    "func-call-spacing": 2,
    "func-name-matching": 2,
    "func-names": 2,
    "func-style": [
        2,
        "declaration",
        {
            "allowArrowFunctions": true
        }
    ],
    // "id-blacklist": [
    //     2,
    //     "e",
    //     "er",
    //     "err",
    //     "cb"
    // ],
    // "id-length": [2, { "min": 2, "exceptions": ["_"]}],
    "indent": [
        "error",
        2
    ],
    "jsx-quotes": 2,
    "key-spacing": 2,
    "keyword-spacing": 2,
    "line-comment-position": 2,
    "linebreak-style": 2,
    "lines-around-comment": 2,
    "max-depth": [
        2,
        3
    ],
    "max-len": [
        2,
        {
            "code": 120,
            "ignoreUrls": true,
            "ignoreStrings": true,
            "ignoreTemplateLiterals": true,
            "ignoreRegExpLiterals": true
        }
    ],
    "max-lines": [
        2,
        {
            "max": 500,
            "skipBlankLines": true,
            "skipComments": true
        }
    ],
    "max-nested-callbacks": [
        "error",
        3
    ],
    "max-statements": [
        "error",
        20
    ],
    "new-cap": 2,
    "new-parens": 2,
    "newline-per-chained-call": [
        "error",
        {
            "ignoreChainWithDepth": 2
        }
    ],
    "no-inline-comments": 2,
    "no-lonely-if": 2,
    "no-mixed-operators": 2,
    "no-multi-assign": 2,
    "no-multiple-empty-lines": 2,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-tabs": 2,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": [
        2,
        {
            "allow": [
                "_id"
            ]
        }
    ],
    "no-unneeded-ternary": 2,
    "no-whitespace-before-property": 2,
    "nonblock-statement-body-position": 2,
    "object-curly-spacing": [
        2,
        "always"
    ],
    "object-property-newline": [
        2,
        {
            "allowMultiplePropertiesPerLine": true
        }
    ],
    "one-var-declaration-per-line": 2,
    "operator-assignment": 2,
    "operator-linebreak": [
        2,
        "before"
    ],
    "padded-blocks": [
        2,
        "never"
    ],
    "quote-props": [
        2,
        "as-needed"
    ],
    "quotes": [
        2,
        "single",
        "avoid-escape"
    ],
    "semi": [
        2,
        "never"
    ],
    "semi-spacing": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [
        2,
        "never"
    ],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": 2,
    "template-tag-spacing": [
        2,
        "always"
    ],
    "arrow-spacing": 2,
    "generator-star-spacing": 2,
    "no-confusing-arrow": [
        "error",
        {
            "allowParens": true
        }
    ],
    "no-duplicate-imports": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": [
        2,
        {
            "allowNamedFunctions": true
        }
    ],
    "prefer-const": 2,
    "prefer-destructuring": 2,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "rest-spread-spacing": 2,
    "sort-imports": 2,
    "symbol-description": 2,
    "template-curly-spacing": 2,
    "yield-star-spacing": 2,
    "eqeqeq": 2        
    },
    "globals": {
        "$": true,
        "YT": true
    } 
}